create table public.grade_4_students (
  "Number" numeric not null,
  "Surname" text null,
  "Name" text null,
  "Full Name" text null,
  "Mother" text null,
  "Mother Cell" text null,
  "Father" text null,
  "Father Cell" text null,
  "Grade" numeric null,
  user_role public.user_role null default 'student'::user_role,
  constraint grade4_students_pkey primary key ("Number"),
  constraint grade4_students_Number_key unique ("Number")
) TABLESPACE pg_default;


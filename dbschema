create table public.teachers (
  "Name" text null,
  "Surname" text null,
  "Full name" text null,
  "Email" text not null,
  user_role text null,
  constraint teachers_pkey primary key ("Email")
) TABLESPACE pg_default;

create table public.submissions (
  id bigint generated by default as identity not null,
  created_at timestamp with time zone not null default now(),
  
  -- Session Info
  date date not null,
  grade text not null,
  subject_id uuid not null,
  subject_name text null, -- Denormalized for easier display
  
  -- Teacher Link
  submitted_by_email text not null references public.teachers("Email"),
  submitted_by_name text null,
  
  -- Student Arrays
  student_numbers numeric[] default '{}', -- All students in class
  student_names text[] default '{}',      -- Corresponds to numbers
  
  -- Status Buckets (Arrays of Student Numbers)
  present_students numeric[] default '{}',
  absent_students numeric[] default '{}',
  late_students numeric[] default '{}',
  excused_students numeric[] default '{}',
  blocked_students numeric[] default '{}',
  
  notes text null,
  
  constraint submissions_pkey primary key (id)
) TABLESPACE pg_default;

